{
    "name": "graceas/php-retry",
    "description": "A library for retrying actions that can fail.",
    "keywords": [ "Retry", "Retrying"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Gorelov",
            "email": "grac.ga@gmail.com",
            "homepage": "https://github.com/Graceas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Retry": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
